{
  "manifest_version": 3,
  "name": "ChatGPT Sent Messages Viewer",
  "version": "1.0",
  "description": "Shows all messages you've sent to ChatGPT.",
  "permissions": ["storage", "scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://chatgpt.com/*"],
      "js": ["content.js"]
    }
  ]
}